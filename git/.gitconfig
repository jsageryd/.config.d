[user]
	name = Johan Sageryd
	email = j@1616.se
[core]
	excludesfile = ~/.config.d/git/.gitexclude
	pager =
	editor = vim
[advice]
	detachedHead = false
[alias]
	a = add
	ci = commit
	co = checkout
	cp = cherry-pick
	rb = rebase
	ri = rebase --interactive
	st = status
[mergetool]
	keepBackup = false
	prompt = false
[color]
	ui = auto
[sendemail]
	smtpserver = smtp.googlemail.com
	smtpserverport = 465
	smtpencryption = ssl
	smtpuser = j@1616.se
[push]
	default = simple
[rebase]
	autosquash = true
[diff]
	tool = p4merge
[difftool]
	prompt = false
[rerere]
	enabled = true
[instaweb]
	httpd = webrick
[fetch]
	prune = true
[status]
	short = true
	branch = true
